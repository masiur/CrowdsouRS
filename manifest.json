{
  "manifest_version": 2,

  "name": "Trust & Reputation System",
  "description": "This extension catches all the link for the current page",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.1.1.min.js", "myscript.js"],
      "run_at": "document_end" // ‘document_end’ after DOM complete but before images 
    }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  }
  
}